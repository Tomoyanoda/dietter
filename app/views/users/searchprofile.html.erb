<%= search_form_for @q, enforce_utf8:false, url: searchprofile_users_path do |f| %>
  <div class="form-group">
    <%= f.label :profile_cont, "Profile" %>
    <%= f.search_field :profile_cont, class: "form-control" %>
  </div>
  <div class="actions"><%= f.submit "Search" %></div>
<% end %>
<br>
<% if @users.any? %>
  <ul class="list-unstyled">
    <% @users.each do |user| %>
      <li class="media">
        <p class="users-profile">
          <% unless user.image? %>
            <%= image_tag 'no_image.png', width:100 %>
          <% else %>  
            <%= image_tag user.image.to_s, width:100 %>
          <% end %>
        </p>
        <div class="media-body">
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to user.profile, user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
<% end %>